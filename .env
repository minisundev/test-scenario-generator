# Azure OpenAI 설정
VITE_AZURE_OPENAI_API_KEY=my_openai_api_key
VITE_AZURE_OPENAI_ENDPOINT=https://my-resource.openai.azure.com/
VITE_AZURE_OPENAI_API_VERSION=2024-02-15-preview

# Azure AI Search 설정
VITE_AZURE_SEARCH_API_KEY=my_search_api_key
VITE_AZURE_SEARCH_ENDPOINT=https://my-search-service.search.windows.net/
VITE_AZURE_SEARCH_INDEX_NAME=security-docs-index

# GPT 모델 설정
VITE_GPT_MODEL_NAME=gpt-4
VITE_EMBEDDING_MODEL_NAME=text-embedding-ada-002

# 기타 설정
VITE_MAX_FILE_SIZE=10485760
VITE_SUPPORTED_CODE_EXTENSIONS=.js,.jsx,.ts,.tsx,.py,.java,.cpp,.c,.cs,.php,.kt,.kts
